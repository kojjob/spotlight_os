    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "flags": "gu"
    }
  ],
  "duplicate_filter": {
    "pattern": "\\[(...|NEW|FIX|DOCS|TEST|BREAKING)\\] (.+)",
    "flags": "gu",
    "target": "$2"
  }
